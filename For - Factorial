#include <stdio.h>

int main() {
    int num, i, fact = 1;

    printf("Ingrese un numero entero positivo: ");
    scanf("%d", &num);

    if(num < 0) {
        printf("Entrada no valida\n");
    } else {
        for(i = 1; i <= num; i++) {
            fact *= i;
        }
        printf("El factorial de %d es: %d\n", num, fact);
    }

    return 0;
}
