#include <stdio.h>

int main() {
    int num, suma = 0;
    printf("Ingrese un numero entero positivo: ");
    scanf("%d", &num);
    while (num >= 0) {
        suma += num;
        printf("Ingrese otro numero entero positivo o un numero negativo para finalizar: ");
        scanf("%d", &num);
    }
    printf("La suma de los numeros ingresados es: %d\n", suma);
    return 0;
}
